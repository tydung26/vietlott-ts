{
  "name": "vietlott-ts",
  "version": "0.1.0",
  "description": "TypeScript implementation for crawling Vietnamese lottery data",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "crawl": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "vietlott",
    "lottery",
    "crawler",
    "typescript"
  ],
  "author": "Joseph Dung",
  "license": "MIT",
  "dependencies": {
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
